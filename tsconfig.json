{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "outDir": "./dist",
    "declaration": true,
    "module": "commonjs",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    // "noFallthroughCasesInSwitch": true,
    // "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
    "jsx": "react-jsx",
  },
  "include": [
    "src"
  ],
  
}
